{
  "name": "fm-testimonials-grid-section",
  "version": "0.1.0",
  "license": "MIT",
  "author": "p1t1ch <belobeev.kirill@gmail.com>",
  "scripts": {
    "dev": "gatsby develop -H 0.0.0.0",
    "build": "gatsby build",
    "start": "gatsby serve -H 0.0.0.0 -p 8000",
    "lint": "eslint . --ext .tsx,.ts,.mdx,.js --fix",
    "format": "prettier . --write --ignore-path .gitignore",
    "type-check": "tsc --noEmit",
    "validate": "npm run lint && npm run format && npm run type-check",
    "clean": "gatsby clean",
    "stats": "open ./public/report.html"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run type-check"
    }
  },
  "lint-staged": {
    "*.{tsx,ts,mdx,js}": [
      "npm run lint",
      "npm run format"
    ]
  },
  "eslintIgnore": [
    "node_modules",
    "public"
  ],
  "browserslist": [
    "last 2 Chrome major versions",
    "last 2 Firefox major versions",
    "last 2 Safari major versions",
    "last 2 Edge major versions",
    "last 2 ChromeAndroid major versions",
    "last 2 iOS major versions"
  ],
  "dependencies": {
    "@fontsource/barlow-semi-condensed": "^4.1.0",
    "@svgr/webpack": "^5.5.0",
    "autoprefixer": "^10.2.1",
    "focus-visible": "^5.2.0",
    "gatsby": "^2.30.3",
    "gatsby-alias-imports": "^1.0.6",
    "gatsby-image": "^2.9.0",
    "gatsby-plugin-manifest": "^2.10.0",
    "gatsby-plugin-offline": "^3.8.0",
    "gatsby-plugin-postcss": "^3.5.0",
    "gatsby-plugin-react-helmet": "^3.8.0",
    "gatsby-plugin-sharp": "^2.12.2",
    "gatsby-plugin-svgr": "^2.1.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.18",
    "gatsby-source-filesystem": "^2.9.1",
    "gatsby-transformer-sharp": "^2.10.1",
    "postcss": "^8.2.4",
    "postcss-focus-visible": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.7",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "babel-preset-gatsby": "^0.10.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "serve": "^11.3.2",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">= 10.14.2"
  }
}
